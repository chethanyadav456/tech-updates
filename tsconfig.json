{
	"compilerOptions": {
		"outDir": "./dist",
		"module": "commonjs",
		"target": "esnext",
		"lib": ["webworker", "dom", "es6"],
		"alwaysStrict": true,
		"strict": true,
		"noImplicitAny": false,
		"preserveConstEnums": true,
		"moduleResolution": "node",
		"sourceMap": true,
		"esModuleInterop": true,
		"types": ["@cloudflare/workers-types", "@types/node"]
	},
	"include": ["src"],
	"exclude": ["node_modules", "dist", "test"]
}
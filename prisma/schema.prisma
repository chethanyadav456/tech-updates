generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_DATABASE_URL")
}

model tc_article {
  id       String @id @default(cuid())
  atitleId Int
}

model user {
  id        String @id @default(cuid())
  userId    String @unique
  channelId String
}
